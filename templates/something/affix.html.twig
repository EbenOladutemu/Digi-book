            {# <li><a href="#author" class="scrollto">Author</a></li>  #}
            <li><a href="{{path('home')}}" class="scrollto">Home</a></li>
            <li><a href="#" class="btn btn-green scrollto">Distribute</a></li>
          </ul>
        </div><!-- /.navbar-collapse -->
